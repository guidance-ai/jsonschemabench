            "pattern": "^PB-[0-9]{8}-[a-z|A-Z]+-.*$"
          },
          "version": {
            "description": "PB configuration version",
            "type": "string",
            "pattern": "[0-9]{1,4}.[0-9]{1,4}.[0-9]{1,4}"
